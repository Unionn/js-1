<script type="text/javascript">
function show_confirm()
{
var r=confirm("账号和密码都是123");
if (r==true)
  {
  alert("输出正确");
  }
else 
  {
  alert("输出错误");
  }
}
</script>
</head>
<body>
<input type="button" onclick="show_confirm()" value="确定点击" />
</body>
